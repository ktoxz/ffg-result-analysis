{
    "name": "ffg-result-analysis-gui",
    "version": "1.0.0",
    "description": "Ứng dụng quản lý và hiển thị kết quả xét nghiệm y khoa",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "dev": "nodemon server/index.js",
        "init-db": "node server/init-db.js",
        "client:install": "cd client && npm install",
        "client:dev": "cd client && npm run dev",
        "client:build": "cd client && npm run build",
        "build": "npm run client:build && xcopy client\\dist server\\public /E /I /Y",
        "install:all": "npm install && npm run client:install"
    },
    "keywords": [
        "medical",
        "lab-results",
        "healthcare"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "better-sqlite3": "^9.4.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.3"
    }
}